{
  "server": {
    "id": 1,
    "host": "",
    "queryPort": 27165,
    "rconPort": 21114,
    "rconPassword": "",
    "logFilePath":"",
    "adminLists": [
      {
        "type": "remote",
        "source": "http://yourwebsite.com/Admins.cfg"
      },
      {
        "type": "local",
        "source": "/home/squadserver/SquadGame/ServerConfig/Admins.cfg"
      }
    ]
  },
  "connectors": {
    "discord":{
      "loginToken": "DiscordLoginToken"
    },
    "mysql": {
      "host": "host",
      "port": 3306,
      "username": "sql_username",
      "password": "sql_password",
      "database": "sql_schema_name"
    }
  },
  "plugins": {
    "AdminCamTracker": {
      "enabled": true
    },
    "ChatCommands": {
      "enabled": true,
      "prefix": "!",
      "commands": [
        {
          "command": "woop",
          "type": "warn",
          "response": "WOOOOOOOOP",
          "ignoreChats": []
        },
        {
          "command": "squad4j",
          "type": "warn",
          "response": "This server is running Squad4J.",
          "ignoreChats": []
        },
        {
          "command": "dadmin",
          "type": "warn",
          "response": "Wup developed a silly command again.",
          "ignoreChats": ["ChatAll", "ChatSquad"]
        }
      ]
    },
    "ChatLogger": {
      "enabled": true
    },
    "DevelopmentTestPlugin": {
      "enabled": true
    }
  },
  "logging": {
    "level": "info",
    "colors": {
      "trace": {
        "bold": false,
        "color": "default"
      },
      "debug": {
        "bold": true,
        "color": "green"
      },
      "info":{
        "bold": true,
        "color": "blue"
      },
      "warn": {
        "bold": true,
        "color": "yellow"
      },
      "error": {
        "bold": true,
        "color": "red"
      }
    }
  }
}